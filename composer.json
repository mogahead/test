{
	"repositories": [
        {
            "type": "composer",
            "url": "http://local.packages.cartalyst.com"
        }
    ],
	"require": {
		"illuminate/foundation": "1.1.*",
		"cartalyst/sentry": "dev-master",
		"platform/dashboard": "dev-master",
		"platform/developers": "dev-master",
		"platform/localization": "dev-master",
		"platform/menus": "dev-master",
		"platform/operations": "dev-master",
		"platform/pages": "dev-master",
		"platform/settings": "dev-master",
		"platform/themes": "dev-master",
		"platform/users": "dev-master",
		"rmccue/requests": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/tests/TestCase.php",
			"vendor/platform/foundation/src/migrations",
			"extensions/platform/dashboard/database/migrations",
			"extensions/platform/developers/database/migrations",
			"extensions/platform/localization/database/migrations",
			"extensions/platform/menus/database/migrations",
			"extensions/platform/operations/database/migrations",
			"extensions/platform/pages/database/migrations",
			"extensions/platform/settings/database/migrations",
			"extensions/platform/themes/database/migrations",
			"extensions/platform/users/database/migrations",

			"extensions/platform/pages/controllers"
		],
		"psr-0": {
			"Cartalyst\\Api": "vendor/cartalyst/api/src",
			"Platform\\Extensions": "vendor/platform/extensions/src",
			"Platform\\Foundation": "vendor/platform/foundation/src",
			"Platform\\Operate": "vendor/platform/operate/src"
		},
		"files": [
			"vendor/platform/foundation/src/helpers.php"
		]
	},
	"minimum-stability": "dev"
}