{
	"repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
	"require": {
		"illuminate/foundation": "1.1.*",
		"cartalyst/sentry": "dev-master",
		"platform/developers": "dev-master",
		"platform/localization": "dev-master",
		"platform/menus": "dev-master",
		"platform/operations": "dev-master",
		"platform/pages": "dev-master",
		"platform/settings": "dev-master",
		"platform/themes": "dev-master",
		"platform/users": "dev-master",
		"rmccue/requests": "dev-master",
		"bshaffer/oauth2-server-php": "dev-develop",
		"bshaffer/oauth2-server-httpfoundation-bridge": "dev-master",
		"friendsofsymfony/rest": "0.8.x-dev"
	},
	"autoload": {
		"classmap": [
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/tests/TestCase.php",
			"vendor/platform/foundation/src/migrations"
		],
		"psr-0": {
			"Cartalyst\\Api": "vendor/cartalyst/api/src",
			"Platform\\Extensions": "vendor/platform/extensions/src",
			"Platform\\Foundation": "vendor/platform/foundation/src",
			"Platform\\Operate": "vendor/platform/operate/src"
		},
		"files": [
			"vendor/platform/foundation/src/helpers.php"
		]
	},
	"minimum-stability": "dev"
}