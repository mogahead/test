{
	"repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
	"require": {
		"cartalyst/extensions": "dev-master",
		"platform/foundation": "dev-master",
		"illuminate/foundation": "1.2.*",
		"jasonlewis/basset": "dev-master",
		"natxet/CssMin": "dev-master",
		"leafo/lessphp": "dev-master",
		"cartalyst/nesty": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Cartalyst\\Themes": "vendor/cartalyst/themes/src",
			"Platform\\Operate": "vendor/platform/operate/src"
		}
	},
	"minimum-stability": "dev"
}